<div class = "container">
  <div class ="staticheaderspacing"></div>
  <% if current_user == @project.user || @project.projectreviewed == true || current_user.try(:admin?) %>

      <!-- Header for the project show page -->
      <%= render 'header' %>

      <div class="projectbackground">
      <div class="projectbackgroundresize">  
      <!-- Form layout for the project -->
      <div class="embed-container">
          <%= raw(youtube_embed(@project.youtubelink)) %>
      </div>

      

      <div class="row">
   
          <div class="span4">
            <%= render 'mainsidebar' %>
          </div>

          <div class="span8">
            <%= render 'maincontent' %>
          </div>
      
      </div>

      <!-- Show Review Comments -->
      <% if @project.projectreviewed !=true %>
          <%= render 'review_comments/reviewcommentlisting' %>
      <% end %>

      </div>
      </div>

  <% end %>
</div>